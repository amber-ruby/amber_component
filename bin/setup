#!/usr/bin/env bash
set -euo pipefail
IFS=$'\n\t'
set -vx

PROJECT_ROOT=$PWD
if $CI; then
  bundle config --local path $PROJECT_ROOT/vendor/bundle
fi

bundle install

cd test/dummy/rails_7

if $CI; then
  bundle config --local path $PROJECT_ROOT/vendor/bundle
fi
bundle install

# Do any other automated setup that you need to do here
